<%= simple_form_for(@client_professional) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :professional %>
    <%= f.association :client %>
    <%= f.input :name %>
    <%= f.input :creator %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
